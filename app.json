{
  "expo": {
    "name": "Farm to Plate",
    "slug": "ecommerce-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/farmtoplate.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/farmtoplate.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need to access your camera to take pictures.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload images."
      }
    },
    "android": {
      "package": "com.teamvatte.farmtoplate",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/farmtoplate.png"
      },
      "jsEngine": "hermes",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "bc016606-a7b4-4e28-a78c-f7b260da1ff8"
      }
    }
  }
}
